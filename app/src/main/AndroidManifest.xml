<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.thienphan.supportstudent">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.project.thienphan.timesheet.View.MainActivity"
            android:label="@string/timesheet">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.project.thienphan.timesheet.View.CreateTimesheet"
            android:label="Thêm thời khoá biểu"
            android:parentActivityName="com.project.thienphan.timesheet.View.MainActivity" />
        <activity android:name="com.project.thienphan.timesheet.View.MainActivityGV" />
        <activity
            android:name="com.project.thienphan.timesheet.View.CreateTimesheetGV"
            android:label="Thêm lớp học"
            android:parentActivityName="com.project.thienphan.timesheet.View.MainActivityGV"></activity>
    </application>

</manifest>